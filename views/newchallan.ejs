                    </div>
                </div>
                <div class="form-group">
                    <label for="locationID" class="col-sm-3 control-label">Location ID*</label>
                    <div class="col-sm-9">
                        <input type="text" name="locationid" placeholder="Location ID" class="form-control">
                    </div>
                </div>


               <!-- /.form-group -->
                <div class="form-group">
                    <div class="col-sm-9 col-sm-offset-3">
                        <span class="help-block">*Required fields</span>
                    </div>
                </div>
