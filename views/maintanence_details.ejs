                </div>
              </li>

            </ul>
          </div>
          <ul class="navbar-nav">
            <li class="nav-item ">
              <a class="nav-link signout" href="login">Sign Out</a>
            </li>
          </ul>

        </nav>


      </section>
    </div>

    <div class="table table-striped table-bordered table-hover">
      <table id="myTable">
          <thead>
              <tr="row100 head">

                  <th>Maintenance Start Time</th>
                  <th>Maintenance End Time</th>
                  <th>Maintenance Type</th>
                  <th>Reason For Maintenance</th>
                  <th>Cooperation</th>
                  <th>Panchayat</th>
                  <th>State</th>
                  <th>District</th>
                  <th>Taluk</th>
                  <th>Landmark</th>

              </tr>
          </thead>
          <tbody>
            <% var i=0; %>
                <% var str="singlerow" %>
                <% var c="Cooperation" %>
                <% var p="Panchayat" %>
                <% var s="State" %>
                <% var d="District" %>
                <% var t="Taluk" %>
                <% var l="Landmark" %>
            <% under_maintanence.forEach(function( eachmain ){ %>
            <%if(i==0) { %>
            <% str=str.concat(i) %>
            <% c=c.concat(i) %>
            <% p=p.concat(i) %>
            <% s=s.concat(i) %>
            <% d=d.concat(i) %>
            <% t=t.concat(i) %>
            <% l=l.concat(i) %>
            <% } %>

              <tr id=<%= str %> class="rowdis">
                  <td><%= eachmain.Maintenance_Start_Time %> </td>
                  <td><%= eachmain.Maintenance_End_Time %> </td>
                  <td><%= eachmain.Maintenance_Type%></td>
                  <td><%= eachmain.Reason_For_Maintenance %></td>
                  <td id=<%= c %>><%= eachmain.Cooperation %> </td>
                  <td id=<%= p %>><%= eachmain.Panchayat %></td>
                  <td id=<%= s %>><%= eachmain.State %> </td>
                  <td id=<%= d %>> <%= eachmain.District %>  </td>
                  <td id=<%= t %>><%= eachmain.Taluk %> </td>
                  <td id=<%= l %>><%= eachmain.Landmark %></td>
              </tr>
              <% str=str.replace(i-1,i) %>
              <% c= c.replace(i-1,i) %>
              <% p=p.replace(i-1,i)%>
              <% s=s.replace(i-1,i) %>
              <% d=d.replace(i-1,i) %>
              <% t=t.replace(i-1,i) %>
              <% l=l.replace(i-1,i) %>
              <% i=i+1 %>
              <% console.log(c) %>
            <% }) %>
          </tbody>
      </table>
  </div>


  <script src="maps.js" charset="utf-8"></script>

  </body>
</html>
